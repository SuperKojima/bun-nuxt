{
    "name": "codespace-nuxt",
    "dockerComposeFile": "./docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/var/www/html",
    "remoteUser": "www-data",
    "forwardPorts": [3000],
    "portsAttributes": {
        "3000": {
            "label": "app"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "dbaeumer.vscode-eslint"
            ],
            "settings": {
                "eslint.enable": true
            }
        }
    }
}
